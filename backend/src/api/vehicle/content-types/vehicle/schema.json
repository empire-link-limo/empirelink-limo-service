{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "gallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "capacity": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feature.feature",
      "inversedBy": "vehicles"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    }
  }
}
